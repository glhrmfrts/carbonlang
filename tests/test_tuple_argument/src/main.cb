import std::io;
import std::string;
import std::rawstring;

func sum_tuple(t: {int, int, int}): int {
    return t[0] + t[1] + t[2];
}

func main(args: [*]String): int {
    let a = {3,4,5};
    let ares = sum_tuple(a);
    if (ares == 3+4+5) {
        println("[OK] test_tuple_argument");
        return 1;
    }
    else {
        println("[NOK] test_tuple_argument");
        return 0;
    }
}