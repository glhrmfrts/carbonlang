// example vector math for parser test

type Vector2 = {x:float, y:float};

func add(a: Vector2, b: Vector2): Vector2 {
	return a + b;
}

func sub(a: Vector2, b: Vector2): Vector2 {
	return a - b;
}

func mul(a: Vector2, b: Vector2) {
	return a * b;
}

