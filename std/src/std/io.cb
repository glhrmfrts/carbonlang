import std::runtime::io as rtio
import std::string

func print(str: string) {
    rtio::write(str->as_slice());
}

func print(str: []pure uint8) {
    rtio::write(str);
}

func println(str: string) {
    rtio::write(str->as_slice());
    rtio::write("\n");
}

func println(str: []pure uint8) {
    rtio::write(str);
    rtio::write("\n");
}